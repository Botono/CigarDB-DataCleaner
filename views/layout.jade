!!!
html(ng-app="CigarDBDataCleaner")
    head
        meta(charset='utf8')
        base(href='/')
        title CigarDB Data Cleanup
        link(rel='stylesheet', href='/css/app.css')
        style
            body {
                padding-top: 60px;
            }

            @media (max-width: 980px) {
                body {
                    padding-top: 0;
                }
            }
    body

        div(class='navbar navbar-fixed-top', bs-navbar)
            div(class='navbar-inner')
                div(class='container-fluid')
                    a(class='brand', href='#') CigarDB Data Cleanup
        div(class='container-fluid')
            div(class='row-fluid')
                block content
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js')
    script(src='js/lib/angular/angular.js')
    script(src='js/lib/angular/angular-resource.js')
    script(src='js/app.js')
    script(src='js/services.js')
    script(src='js/controllers.js')
    script(src='js/filters.js')
    script(src='js/directives.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.1/js/bootstrap.min.js')
    script(src='js/lib/ui-bootstrap-0.3.0.min.js')
    script(src='js/lib/angular-ui-utils.min.js')
